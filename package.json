{
  "name": "timeline-client",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "scripts/build.sh",
    "prepublish": "typings install && npm run build",
    "watch-client": "watchify src/client/index.ts -p [ tsify --project src/client/tsconfig.json ] -o build/client/js/index.js --external react --external react-dom --external classnames --external react-router -v",
    "watch-stylus": "stylus --watch --out ./build/client/css/index.css ./src/client/stylus/index.styl",
    "client-server": "scripts/server.js",
    "watch-server": "tsc -w -p src/server/",
    "server": "node build/server",
    "database": "docker run --rm -e POSTGRES_USER=docker -e POSTGRES_PASSWORD=docker -e PGDATA=/pgdata -v timeline-data:/pgdata -p 5432:5432 gijsjan/postgresql:1.0"
  },
  "types": "src/interfaces/interfaces.d.ts",
  "devDependencies": {
    "browser-sync": "^2.14.0",
    "browserify": "^13.1.0",
    "connect-modrewrite": "^0.9.0",
    "lodash.debounce": "^4.0.8",
    "proxy-middleware": "^0.15.0",
    "stylus": "^0.54.5",
    "tsify": "^1.0.3",
    "tslint": "^3.13.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.2",
    "url": "^0.11.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "hire-forms-autocomplete-list": "^2.0.1",
    "hire-forms-checkbox": "^1.0.3",
    "hire-forms-input": "^2.1.0",
    "hire-forms-select": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "pg": "^6.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  }
}
