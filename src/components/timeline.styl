.timeline
	background timelineBlue
	height 100%

	& > header
		margin 0 auto
		text-align center
		width 50%
		position relative
		z-index 1

		h2
			font-size 3em
			margin 0
			padding 0

	.dev
		position fixed
		padding 1%
		right 0
		bottom 32px
		background-color rgba(0, 0, 0, 0.5)
		border-top-left-radius 3px
		border-bottom-left-radius 3px
		color white
		font-size 0.6em

